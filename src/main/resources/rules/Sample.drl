package com.rozzie.processor

import org.rozzie.processor.utils.Util
import java.time.Duration;
import org.json.JSONObject;

rule "flightDepartureDelay"
  when
  $json : JSONObject ()
  eval(Util.timeGreaterThan($json.get("scheduled_departure_time"),$json.get("estimated_departure_time")))
  then
  System.out.println("Your flight departure time is delayed by X number of hours");
end